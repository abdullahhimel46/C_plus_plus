// StringInputAndDisplay
#include <bits/stdc++.h> 
using namespace std;     

int main()
{
    int n;
    cin >> n; // Taking user input for the number of strings to be entered.

    getchar(); // Clearing the newline character from the input buffer.

    vector<string> s(n); // Creating a vector 's' of strings with size 'n'.

    for (int i = 0; i < n; i++)
    {
        getline(cin, s[i]); 
        // Taking input line-by-line and storing it in the vector 's'.
    }

    for (string val : s) // Iterating through each string stored in vector 's'.
    {
        cout << val << endl; // Printing each string followed by a newline.
    }

    return 0; 
}
