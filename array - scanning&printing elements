#include <iostream>
using namespace std;
int main()
{
    int n;
    cout << "enter n: ";
    cin >> n;
    int a[n];

    // scanning elements
    cout << "enter elements: ";
    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
    }

    // printing elements
    cout << "entred elemnts: ";
    for (int i = 0; i < n; i++)
    {
        cout << a[i] << " ";
    }
}
