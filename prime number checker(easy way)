#include <iostream>
using namespace std;

int main() {

int n,t;
cin>>t;
while(t--){
  int cnt=0;
  cin>>n;

  for(int i=1; i<=n; i++){
    if(n%i==0)
        cnt++;
  }

    //A prime number is a natural number that has only 1 and itself as factors
    //So, prime number has total 2 multiple => cnt==2
  if (cnt==2)
    cout << n << " eh primo\n";
  else
    cout << n << " nao eh primo\n";
}
  return 0;
}
