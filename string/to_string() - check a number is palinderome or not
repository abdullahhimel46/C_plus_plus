//check a number is palinderome or not
#include <bits/stdc++.h>
using namespace std;
int main(){
    int n; cin>>n;
    string s = to_string(n);
    string s2=s;
    reverse(s.begin(),s.end());

    if(s == s2)
        cout<<"palindrome";
    else  
        cout<<"not palindrome";
        
    return 0; 
}
