#include <bits/stdc++.h>
using namespace std;

const char nl = '\n';

void solve()
{
    int n = 6;
    int mat[n][n];

    for (int i = 0; i < n; i++)
    {
        cout << "*";
    }
    cout << nl;
}

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);
    solve();
    return 0;
}
