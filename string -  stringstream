#include <bits/stdc++.h>
using namespace std;
int main()
{
    string s = "Hi, I am Abdullah";
    stringstream ss(s);
    // constructor ob(parameter);
    string word;
    int cnt = 0;
    while (ss >> word)
    {
        cout << word << endl;
        cnt++;
    }
    cout << "total words: " << cnt << endl;
    return 0;
}
