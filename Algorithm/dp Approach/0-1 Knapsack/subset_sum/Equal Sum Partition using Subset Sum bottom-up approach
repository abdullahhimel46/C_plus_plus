//** BISMILLAHIR ROHMANIR ROHIM **
// Equal Sum Partition using Subset Sum bottom-up approach
/*
Input
3
1 2 5
Output: NO
Input
4
1 5 11 5
Output
YES

*/

#include <bits/stdc++.h>
using namespace std;

const char nl = '\n';
typedef vector<int> vi;
#define py cout << "YES" << endl
#define pn cout << "NO" << endl
#define fori for (int i = 0; i < n; i++)
#define forj for (int j = 0; j < n; j++)

int main()
{
    ios::sync_with_stdio(false);
    cin.tie(0);

    int n;
    cin >> n;
    vector<int> nums(n);
    int sum = 0;
    fori
    {
        cin >> nums[i];
        sum += nums[i];
    }

    // early termination
    if (sum % 2 != 0)
    {
        pn; // as sum is a odd number
        return 0;
    }

    // if sum is an even number, then it can be devied into to partition
    int s = sum / 2;
    /*
    For intput:
    3
    1 2 5

    sum=1+2+5=8
    s=4; so, we've to find subset of 4
    */

    bool dp[n + 1][s + 1];

    for (int i = 0; i < n + 1; i++)
        dp[i][0] = true; // set 1st col to T

    for (int j = 1; j < s + 1; j++)
        dp[0][j] = false; // set 1st row to F

    for (int i = 1; i < n + 1; i++)
    {
        // i -> index/element no
        for (int j = 1; j < s + 1; j++)
        {
            // j -> sum
            // prevent any negative index access
            if (j >= nums[i - 1])
                dp[i][j] = dp[i - 1][j - nums[i - 1]] || dp[i - 1][j];
            else
                dp[i][j] = dp[i - 1][j];
            /// explanation:
            // bool include = dp[i - 1][j - nums[i - 1]];
            // bool exclude = dp[i - 1][j];
            // dp[i][j] = include || exclude;
        }
    }

    /// final dp table
    // for (int i = 0; i <= n; i++)
    // {
    //     for (int j = 0; j <= s; j++)
    //     {
    //         cout << dp[i][j] << " ";
    //     }
    //     cout << nl;
    // }

    // result will be in the last cell of the dp table
    if (dp[n][s]) // subset is found
        py;
    else
        pn;

    return 0;
}
