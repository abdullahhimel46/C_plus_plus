// STL Queue
#include <bits/stdc++.h>
using namespace std;
int main()
{
    queue<int> q;
    int n;
    cin >> n;

    // taking input for n number of elements
    while (n--)
    {
        int x;
        cin >> x;
        q.push(x);
    }

    cout << "Number of elements: ";
    cout << q.size() << endl;
    // output
    while (!q.empty())
    {
        cout << q.front() << endl;
        q.pop();
    }
    return 0;
}
