/*
rtrn_typ | fun_name(parameter) | result
---------------------------------------
     int | isalpha(int ch)     | 1(true)    
----------------------------------------
     int | isdigit(int ch)     | 1(true)
----------------------------------------
     int | isalnum(int ch)     | 1(true)
----------------------------------------
*/

#include <bits/stdc++.h>
using namespace std;
int main()
{
    char c;
    cin >> c;

    if (isalpha(c))
    {
        cout << c << " is an alphabet" << endl;
    }
    else
    {
        cout << c << " isn't an alphabet" << endl;
    }

    if (isdigit(c))
    {
        cout << c << " is a digit" << endl;
    }
    else
    {
        cout << c << " isn't a digit" << endl;
    }

    if (isalnum(c))
    {
        cout << c << " is an alpha or digit" << endl;
    }
    else
    {
        cout << c << " isn't an alpha or digit" << endl;
    }
    return 0;
}
