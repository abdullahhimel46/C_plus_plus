// VectorElementErasure
#include <bits/stdc++.h>
using namespace std;

int main()
{
    // Initializing a vector 'v' with elements
    vector<int> v = {1, 2, 3, 4, 5, 6};

    // Displaying original elements of vector 'v'
    cout << "Original elements: " << endl;
    for (int x : v)
    {
        cout << x << " ";
    }
    cout << endl;

    // Erasing elements at positions 1 to 2 (excluding position 3) in vector 'v'
    cout << "After erasing some elements: " << endl;
    v.erase(v.begin() + 1, v.begin() + 3);

    // Displaying elements of vector 'v' after erasing
    for (int x : v)
    {
        cout << x << " ";
    }
    cout << endl;
    return 0;
}
